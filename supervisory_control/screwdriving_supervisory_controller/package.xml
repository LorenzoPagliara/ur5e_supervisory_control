<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>screwdriving_supervisory_controller</name>
  <version>0.0.0</version>
  <description>This package implements a supervisory controller for screwing operations</description>
  <maintainer email="lpagliara@unisa.it">lpagliara</maintainer>
  <license>BSD</license>

  <buildtool_depend>ament_cmake</buildtool_depend>
  <depend>acg_control_msgs</depend>
  <depend>controller_manager_msgs</depend>
  <depend>moveit_ros_planning_interface</depend>
  <depend>moveit_visual_tools</depend>
  <depend>rclcpp</depend>
  <depend>supervisory_control_rviz_msg</depend>

  <exec_depend>supervisory_control_rviz_panel</exec_depend>

  <test_depend>ament_lint_auto</test_depend>
  <test_depend>ament_lint_common</test_depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
