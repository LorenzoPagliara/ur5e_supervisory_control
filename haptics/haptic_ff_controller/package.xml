  <depend>pose_broadcaster</depend>
  <depend>rclcpp</depend>
  <depend>rclcpp_lifecycle</depend>
  <depend>realtime_tools</depend>
  <depend>ros2_control</depend>
  <depend>tf2</depend>
  <depend>tf2_eigen</depend>
