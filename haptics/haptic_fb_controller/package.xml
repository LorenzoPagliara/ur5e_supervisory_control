<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>haptic_fb_controller</name>
  <version>0.0.0</version>
  <description>This package contains the haptic feedback controller.</description>
  <maintainer email="eferrentino@unisa.it">Enrico Ferrentino</maintainer>
  <author email="s.paolino6@studenti.unisa.it">Salvatore Paolino</author>
  <license>BSD</license>

  <buildtool_depend>ament_cmake</buildtool_depend>

  <depend>acg_control_msgs</depend>
  <depend>eigen</depend>
  <depend>filters</depend>
  <depend>generate_parameter_library</depend>
  <depend>geometry_msgs</depend>
  <depend>hardware_interface</depend>
  <depend>kinematics_interface</depend>
  <depend>pluginlib</depend>
  <depend>rclcpp</depend>
  <depend>rclcpp_lifecycle</depend>
  <depend>realtime_tools</depend>
  <depend>ros2_control</depend>

  <exec_depend>acg_admittance_controller</exec_depend>
  <exec_depend>gravity_compensation_filter</exec_depend>
  <exec_depend>haptic_forward_force_controller</exec_depend>
  <exec_depend>touch_haptic_device</exec_depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
