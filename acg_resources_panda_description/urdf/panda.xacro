    <child link="$(arg tf_prefix)link6"/>
    <axis xyz="0 0 1"/>
    <limit effort="12" lower="-0.0873" upper="3.8223" velocity="2.8710"/>
  </joint>
  <link name="$(arg tf_prefix)link7">
    <visual>
      <geometry>
