  </joint>
  <joint name="$(arg tf_prefix)hand_joint" type="fixed">
    <parent link="$(arg tf_prefix)link8"/>
    <child link="$(arg tf_prefix)hand"/>
    <origin rpy="0 0 -0.785398163397" xyz="0 0 0"/>
  </joint>
  <link name="$(arg tf_prefix)hand">
    <visual>
      <geometry>
