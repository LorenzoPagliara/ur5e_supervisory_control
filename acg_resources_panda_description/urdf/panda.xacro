<?xml version="1.0"?>
<robot name="panda" xmlns:xacro="http://www.ros.org/wiki/xacro">
  <xacro:include filename="$(find acg_resources_panda_description)/urdf/inertial.xacro"/>
  <xacro:arg name="tf_prefix" default="panda/"/>

  <link name="world"/>

  <joint name="world_to_base" type="fixed">
    <origin rpy="0 0 0" xyz="0 0 0"/>
    <parent link="world"/>
    <child link="$(arg tf_prefix)link0"/>
  </joint>

  <link name="$(arg tf_prefix)link0">
    <visual>
      <geometry>
        <mesh filename="package://acg_resources_panda_description/meshes/visual/link0.dae"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://acg_resources_panda_description/meshes/collision/link0.stl"/>
      </geometry>
    </collision>
    <xacro:inertial_props name="link0"/>
  </link>
  <link name="$(arg tf_prefix)link1">
    <visual>
      <geometry>
        <mesh filename="package://acg_resources_panda_description/meshes/visual/link1.dae"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://acg_resources_panda_description/meshes/collision/link1.stl"/>
      </geometry>
    </collision>
    <xacro:inertial_props name="link1"/>
  </link>
  <joint name="$(arg tf_prefix)joint1" type="revolute">
    <safety_controller k_position="100.0" k_velocity="40.0" soft_lower_limit="-2.8973" soft_upper_limit="2.8973"/>
    <origin rpy="0 0 0" xyz="0 0 0.333"/>
    <parent link="$(arg tf_prefix)link0"/>
    <child link="$(arg tf_prefix)link1"/>
    <axis xyz="0 0 1"/>
    <limit effort="87" lower="-2.9671" upper="2.9671" velocity="2.3925"/>
  </joint>
  <link name="$(arg tf_prefix)link2">
    <visual>
      <geometry>
        <mesh filename="package://acg_resources_panda_description/meshes/visual/link2.dae"/>
