      </geometry>
    </collision>
    <xacro:inertial_props name="leftfinger"/>
  </link>
  <link name="$(arg tf_prefix)rightfinger">
    <visual>
      <origin rpy="0 0 3.14159265359" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://acg_resources_panda_description/meshes/visual/finger.dae"/>
      </geometry>
    </visual>
    <collision>
      <origin rpy="0 0 3.14159265359" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://acg_resources_panda_description/meshes/collision/finger.stl"/>
      </geometry>
    </collision>
    <xacro:inertial_props name="rightfinger"/>
  </link>
  <joint name="$(arg tf_prefix)finger_joint1" type="prismatic">
    <parent link="$(arg tf_prefix)hand"/>
    <child link="$(arg tf_prefix)leftfinger"/>
    <origin rpy="0 0 0" xyz="0 0 0.0584"/>
    <axis xyz="0 1 0"/>
    <limit effort="20" lower="0.0" upper="0.04" velocity="0.2"/>
  </joint>
  <joint name="$(arg tf_prefix)finger_joint2" type="prismatic">
    <parent link="$(arg tf_prefix)hand"/>
    <child link="$(arg tf_prefix)rightfinger"/>
    <origin rpy="0 0 0" xyz="0 0 0.0584"/>
    <axis xyz="0 -1 0"/>
    <limit effort="20" lower="0.0" upper="0.04" velocity="0.2"/>
    <mimic joint="$(arg tf_prefix)finger_joint1"/>
  </joint>
