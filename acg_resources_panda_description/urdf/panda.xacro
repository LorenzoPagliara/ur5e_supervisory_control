    <origin rpy="1.57079632679 0 0" xyz="0.0825 0 0"/>
    <parent link="$(arg tf_prefix)link3"/>
    <child link="$(arg tf_prefix)link4"/>
    <axis xyz="0 0 1"/>
    <limit effort="87" lower="-3.1416" upper="0.0873" velocity="2.3925"/>
  </joint>
  <link name="$(arg tf_prefix)link5">
    <visual>
      <geometry>
        <mesh filename="package://acg_resources_panda_description/meshes/visual/link5.dae"/>
